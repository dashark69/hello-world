#include<iostream>
#define max(a,b) a>b?a:b
using namespace std;
int main ()
{
	int k;
	while(cin>>k)
	{
		int att[k];
		int dp[k];
		int len=1;	//子序列长度 
		for(int i=0;i<k;i++)
		{
			cin>>att[i];
			dp[i]=1;
		}
		for(int i=0;i<k;i++)
		{
			for(int j=0;j<i;j++)
			{
				if(att[j]>=att[i] && dp[j]+1>dp[i])
				{
					dp[i]=dp[j]+1;
					if(dp[i]>len)
					{
						len=dp[i];
					}
				}
			}
		}
		cout<<len<<endl;
	}
	return 0; 
}
