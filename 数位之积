int func(int n)
{
	int arr[10];
	memset(arr,0,sizeof(arr));
	int cnt=0;
	int flag=0;
	for(int i=9;i>1;i--)
	{
		if(n%i==0)
		{
			arr[cnt]=i;
			n/=i;
			i=10;
			cnt++;
			flag=1;
		}
	}
	if(flag==0 || n!=1)
		cout<<-1<<endl;
	else
	{
		for(int i=cnt-1;i>=0;i--)
			cout<<arr[i];
		cout<<endl;
	}
}

int main()
{
	int m,q;
	cin>>m;
	for(int i=0;i<m;i++)
	{
		cin>>q;
		if(q<10)
			cout<<q<<endl;
/*		else if(Iszhishu(q))
			cout<<-1<<endl;*/
		else
			func(q);
	}
	
	return 0;
}
